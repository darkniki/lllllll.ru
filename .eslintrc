{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "brace-style": ["error", "1tbs", {"allowSingleLine": false}],
    "indent": ["error", 4,
      {
        "SwitchCase": 1
      }
    ],
    "newline-after-var": ["error", "always"],
    "newline-before-return": "error",
    "require-jsdoc": "off",
    "no-unused-vars": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
